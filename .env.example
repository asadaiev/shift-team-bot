# Telegram Bot Token (required)
BOT_TOKEN=your_bot_token_here

# Database path (optional, default: stats.db)
DB_PATH=stats.db

# Message time estimation (optional)
TYPING_CHARS_PER_MIN=200
SECONDS_OVERHEAD_PER_MSG=2

# FACEIT API (optional, for /elo command)
FACEIT_API_KEY=your_faceit_api_key_here
FACEIT_GAME=cs2
FACEIT_CACHE_TTL_SEC=300
FACEIT_MAX_CONCURRENCY=3
